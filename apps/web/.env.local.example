# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# PostHog Analytics (Optional)
NEXT_PUBLIC_POSTHOG_KEY=your_posthog_key
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# Sentry Error Tracking (Optional)
NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn

# Feature Flags (Optional)
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_PWA=true

# Dev-only: Demo account prefills for /dev/impersonate (Optional)
# These are used ONLY to prefill the client-side form in development.
# Do NOT put real secrets here. Use simple throwaway demo credentials.
NEXT_PUBLIC_DEMO_ADMIN_EMAIL=
NEXT_PUBLIC_DEMO_ADMIN_PASSWORD=
NEXT_PUBLIC_DEMO_PARENT_EMAIL=
NEXT_PUBLIC_DEMO_PARENT_PASSWORD=
NEXT_PUBLIC_DEMO_CHILD_EMAIL=
NEXT_PUBLIC_DEMO_CHILD_PASSWORD=
